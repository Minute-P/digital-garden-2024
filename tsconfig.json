{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/assets/*": ["src/assets/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "./src/env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/utils/cn.js",
    "src/lib/cn.ts"
  ],
  "exclude": ["dist"]
}
